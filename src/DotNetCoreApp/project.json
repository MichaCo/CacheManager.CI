{
  "version": "1.0.0-*",
  "description": "DotNetCoreApp Console Application",
  "authors": [ "Micha" ],

  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "CacheManager.Core": "0.9.0-beta-1294",
    "CacheManager.Microsoft.Extensions.Caching.Memory": "0.9.0-beta-1294",
    "CacheManager.Microsoft.Extensions.Configuration": "0.9.0-beta-1294",
    "CacheManager.Microsoft.Extensions.Logging": "0.9.0-beta-1294",
    "CacheManager.Serialization.Json": "0.9.0-beta-1294",
    "CacheManager.Serialization.ProtoBuf": "0.9.0-beta-1294",
    "CacheManager.StackExchange.Redis": "0.9.0-beta-1294",
    "Microsoft.Extensions.Logging.Debug": "1.0.0"
  },

  "commands": {
    "DotNetCoreApp": "DotNetCoreApp"
  },

  "runtimes": {
    "win7-x64": {}
  },
  "frameworks": {
    "net452": {
      "dependencies": {
        "CacheManager.SystemRuntimeCaching": "0.9.0-beta-1294"
      }
    },
    "netcoreapp1.0": {
      "buildOptions": { "define": [ "NETCORE" ] },
      "dependencies": {
        "Microsoft.NETCore.App": "1.0.0",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.Tasks.Parallel": "4.0.1",
        "System.Console": "4.0.0",
        "System.Runtime": "4.1.0"
      }
    }
  }
}
