    "CacheManager.Microsoft.Extensions.Logging": "0.9.1-beta-1310",
    "CacheManager.Serialization.Json": "0.9.1-beta-1310",
    "CacheManager.Serialization.ProtoBuf": "0.9.1-beta-1310",
    "CacheManager.StackExchange.Redis": "0.9.1-beta-1310",
    "Microsoft.Extensions.Logging.Debug": "1.0.0"
