{
  "version": "1.0.0-*",
  "description": "DotNetCoreApp Console Application",
  "authors": [ "Micha" ],

  "buildOptions": {
    "emitEntryPoint": true,
    "keyFile": "../../key.snk",
    "warningsAsErrors": true,
    "allowUnsafe": false    
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "optimize": false
      }
    },
    "Release": {
      "buildOptions": {
        "optimize": true,
        "define": [ "RELEASE" ]
      }
    }
  },
  "dependencies": {
    "CacheManager.Core": "0.9.3-beta-*",
    "CacheManager.Microsoft.Extensions.Caching.Memory": "0.9.3-beta-*",
    "CacheManager.Microsoft.Extensions.Configuration": "0.9.3-beta-*",
    "CacheManager.Microsoft.Extensions.Logging": "0.9.3-beta-*",
    "CacheManager.Serialization.Json": "0.9.3-beta-*",
    "CacheManager.Serialization.ProtoBuf": "0.9.3-beta-*",
    "CacheManager.StackExchange.Redis": "0.9.3-beta-*",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "buildOptions": { "define": [ "NETCORE" ] },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        },
        "System.Threading.Thread": "4.3.0",
        "System.Threading.Tasks.Parallel": "4.3.0",
        "System.Console": "4.3.0",
        "System.Runtime": "4.3.0"
      }
    }
  }
}
